{
  "compilerOptions": {
    "module": "ESNext",
    "moduleResolution": "Node",
    "target": "esnext",
    "lib": ["es6", "dom"],
    "types": ["cypress", "node", "cypress-real-events"],
    "strict": true,
    "skipLibCheck": true,
    "resolveJsonModule": true,
    "allowJs": true,
    "baseUrl": "./",
    "noEmit": true
  },
  "include": ["**/*.ts"],
  "exclude": ["node_modules"]
}